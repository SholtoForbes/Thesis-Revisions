journal = {IEEE Transactions on Geoscience and Remote Sensing},
keywords = {Constellation deployment,Constellation observing system for meteorology,FORMOSA Satellite Series No. 3 (FORMOSAT-3),Geodesy,Global positioning system (GPS) radio occultation,Ionosphere and climate (COSMIC),Satellite},
