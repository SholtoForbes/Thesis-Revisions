file = {:C$\backslash$:/Users/uqsforb1/Dropbox/phd/papers/Zhang2018.pdf:pdf},
isbn = {0954410017},
issn = {20413025},
journal = {Proceedings of the Institution of Mechanical Engineers, Part G: Journal of Aerospace Engineering},
keywords = {Rocket-based-combined-cycle,dual-rocket-based-combined-cycle,launch trajectory,liquid oxygen/methane staged cycle liquid-rocket e,two-stage-to-orbit launch vehicle},
number = {5},
pages = {944--957},
