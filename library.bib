@article{Smart2012,
abstract = {The supersonic combustion ramjet, or scramjet, is the engine cycle$\backslash$nmost suitable for sustained hypersonic flight in$\backslash$n$\backslash$nthe atmosphere. This paper examines a key decision in the design of$\backslash$nthe inlet or intake of these hypersonic$\backslash$n$\backslash$nairbreathing engines, namely, the level of compression to be performed.$\backslash$nToo much compression can lead to onerous$\backslash$n$\backslash$nsystem level issues including the need for bleed or variable geometry,$\backslash$nwhile too little compression can result in low$\backslash$n$\backslash$ncycle efficiency and poor combustion of fuel. An analysis of the important$\backslash$nfactors that affect the choice of scramjet$\backslash$n$\backslash$ninlet compression ratio has been performed for hydrogen-fueled scramjets$\backslash$nat Mach 6, 8, 10, and 12. It was found that$\backslash$n$\backslash$ncontrary to classical thermodynamic analyses, scramjet cycle efficiency$\backslash$nreaches an optimum at a relatively low$\backslash$n$\backslash$ncompression ratio between 50 and 100 for all Mach numbers. Practical$\backslash$nconstraints related to nonequilibrium flow$\backslash$n$\backslash$neffects, inlet starting, and boundary-layer separation were also shown$\backslash$nto prompt a desire for low compression ratio.$\backslash$n$\backslash$nThe lower limit on compression was found to be set by the need to$\backslash$ncomplete the combustion reaction in the available$\backslash$n$\backslash$nengine length and is therefore dependent on engine scale. On the basis$\backslash$nof these factors it is recommended that$\backslash$n$\backslash$nscramjet inlet compression ratio be set to the minimum that satisfies$\backslash$nthe robust combustion requirement, with the$\backslash$n$\backslash$ncaveat that it not be below 50 in order to maintain high cycle efficiency.$\backslash$nFor typical wind-tunnel-scale engines, this$\backslash$n$\backslash$nresults in a requirement for the inlet to compress airflow entering$\backslash$nthe combustor to a pressure of approximately 1 atm,$\backslash$n$\backslash$n2$\backslash$n$\backslash$nregardless of the flight Mach number.},
author = {Smart, Michael K.},
doi = {10.2514/1.J051281},
file = {:C$\backslash$:/Users/uqsforb1/Dropbox/phd/papers/Smart2012.pdf:pdf},
issn = {0001-1452},
journal = {AIAA Journal},
number = {3},
pages = {610--619},
title = {{How Much Compression Should a Scramjet Inlet Do?}},
url = {http://arc.aiaa.org/doi/10.2514/1.J051281},
volume = {50},
year = {2012}
}
