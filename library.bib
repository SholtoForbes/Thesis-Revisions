@article{Preller2017b,
abstract = {Reduced scale and improved responsiveness will be the technical and economic drivers of future satellite systems. Based on decades of practical experience with rocket-only expendable launch vehicles, current technology is operated close to theoretical limits. Scramjets have an advantage over rocket propulsion in terms of a significantly higher specific impulse. Other benefits of airbreathing propulsion for access-to-space are increased launch flexibility and the possibility of reusable aircraft-like operations. This article describes the use of a three-stage rocket-scramjet-rocket system for transporting payloads of the order of 100 kg to a Sun Synchronous Orbit. The reusable second stage is based on a winged-cone vehicle and is powered by hydrogen fueled scramjets. Analysis of the complete three-stage system was performed based around a fully trimmed trajectory simulation of the second stage. The scramjet powered second stage accelerated from Mach 6.0 to Mach 9.3 with an average net specific impulse of 923 seconds. As a result, the overall system showed a payload mass fraction of 1.26\% to Sun Synchronous orbit, which is significantly higher than expendable rocket based systems of this scale. Nomenclature A = Area (m 2) c = chord D = Diameter (m), Drag (N) f = specific thrust (m/s) g = gravity (m/s 2) h = altitude (km) i w = wing incidence angle (deg) I sp = Specific Impulse (s) I sp\_net = Net Specific Impulse (s) L = length (m), Lift (N) m = mass (kg) M = Mach number p = pressure (kPa) q = dynamic pressure (kPa) R = radius (m) r b = centre body radius (m) s = wing half span (m) $\delta$ = deflection angle (deg) $\Phi$ = forebody half angle (deg) $\gamma$ = flight path angle (deg) $\rho$ = density (kg/m 3) Subscript 0 = free stream 1 = 1 st stage, flow properties entering the scramjet 2 = 2 nd stage 3 = 3 rd stage dry = dry mass p = propellant total = total system * = payload American Institute of Aeronautics and Astronautics 2 t = thickness t = time (s) T = temperature (K), Thrust (N) V = Velocity (m/s) w c = engine capture width (m) $\alpha$ = Angle of attack (deg) $\beta$ = forebody shock angle (deg) Χ = boat tail half angle (deg)},
author = {Preller, Dawid and Smart, Michael K.},
doi = {10.2514/1.A33610},
file = {:C$\backslash$:/Users/uqsforb1/Dropbox/phd/papers/Preller2017a.pdf:pdf},
isbn = {978-1-62410-427-5},
